<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="533.0" prefWidth="886.0" styleClass="bodybg" stylesheets="@../css/background.css" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Design.CheckoutController">
   <children>
      <AnchorPane layoutX="140.0" layoutY="40.0" prefHeight="243.0" prefWidth="661.0" style="-fx-background-color: white;">
         <children>
            <TableView fx:id="otable" prefHeight="243.0" prefWidth="661.0">
              <columns>
                <TableColumn fx:id="Itemcol" prefWidth="163.0" text="Item name" />
                <TableColumn fx:id="descol" prefWidth="336.0" text="Description" />
                  <TableColumn fx:id="qtycol" prefWidth="70.0" text="Quantity" />
                  <TableColumn fx:id="pricecol" prefWidth="91.0" text="Price" />
              </columns>
            </TableView>
         </children>
      </AnchorPane>
      <Button fx:id="Ckbtn" layoutX="349.0" layoutY="463.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="94.0" stylesheets="@../css/button.css" text="Checkout">
         <graphic>
            <ImageView fitHeight="28.0" fitWidth="18.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../icons/checkout%20logo.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <TextField fx:id="totaltxt" layoutX="558.0" layoutY="464.0" prefHeight="25.0" prefWidth="94.0" stylesheets="@../css/textfield2.css" text="Total: " />
   </children>
</AnchorPane>
